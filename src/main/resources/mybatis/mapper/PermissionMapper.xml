<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jigpud.snow.mapper.PermissionMapper">
    <select id="insertIgnore">
        INSERT IGNORE INTO permission (userid, permission) VALUES (#{userid}, #{permission})
    </select>
</mapper>